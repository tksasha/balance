package itemcomponents

import (
	"github.com/tksasha/balance/internal/decorators"
	"github.com/tksasha/balance/internal/models"
)

templ CreatePage(items *decorators.ItemsDecorator, categories *decorators.CategoriesDecorator) {
	@Form(decorators.NewItemDecorator(models.NewItem()), categories)
	<div id="items" hx-swap-oob="true">
		@Table(items)
	</div>
}
